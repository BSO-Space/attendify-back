{
  "name": "attendify-back",
  "version": "0.0.0",
  "description": "backend server for attendify",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "npx prisma generate --schema=./src/prisma/schema.prisma",
    "prisma:generate:release": "DOTENV_CONFIG_PATH=./env.release npx prisma generate --schema=./src/prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --name init --schema=./src/prisma/schema.prisma",
    "prisma:seed": "node --loader ts-node/esm src/prisma/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BSO-Space/attendify-back.git"
  },
  "keywords": [
    "attendify"
  ],
  "author": "BSO-Space",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/BSO-Space/attendify-back/issues"
  },
  "homepage": "https://github.com/BSO-Space/attendify-back#readme",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.0",
    "@types/helmet": "^4.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.1",
    "@types/nodemon": "^1.19.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "nodemon": "^3.1.7",
    "prisma": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@prisma/client": "^6.0.1",
    "@types/body-parser": "^1.19.5",
    "@types/express-validator": "^3.0.2",
    "@types/typescript": "^2.0.0",
    "@types/xlsx": "^0.0.35",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.20",
    "typescript": "^5.5.4"
  }
}
